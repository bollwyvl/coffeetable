{
  "name": "coffeetable",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test-coffee": "mocha --recursive --compilers coffee:coffee-script/register --require coffee-coverage/register-istanbul -R tap src/test/*.coffee.md",
    "test": "mocha --recursive -R tap lib/test/*.js",
    "coverage": "istanbul report",
    "testem": "testem",
    "build": "browserify -t coffeeify --extension='.coffee' src/coffeetable.coffee --standalone coffeetable -o lib/coffeetable.js",
    "build-test": "browserify -t coffeeify --debug --extension='.coffee.md' src/test/index.coffee.md --external coffeetable -o lib/test/index.js",
    "prepublish": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chai": "^3.4.1",
    "coffee-coverage": "^0.7.0",
    "coffee-script": "^1.10.0",
    "coffeeify": "^2.0.1",
    "mocha": "^2.3.4",
    "mocha-cov-reporter": "^1.1.3",
    "mocha-istanbul": "^0.2.0",
    "testem": "^0.9.11"
  }
}
